// Rleud Thin

@font-face {
    font-family: 'Rleud';
    src: url('../fonts/rleud-thin-100-webfont.eot');
    src: url('../fonts/rleud-thin-100-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/rleud-thin-100-webfont.woff') format('woff'),
         url('../fonts/rleud-thin-100-webfont.ttf') format('truetype'),
         url('../fonts/rleud-thin-100-webfont.svg#rleud-thinuploaded_file') format('svg');
    font-weight: 100;
    font-style: normal;
}

